<div
	class="fb-page"
	data-adapt-container-width={{this.adapt_container_width}}
	data-height={{this.height}}
	data-hide-cover={{this.hide_cover}}
	data-hide-cta={{this.hide_cta}}
	data-href={{this.href}}
	data-lazy={{this.lazy}}
	data-show-facepile={{this.show_facepile}}
	data-small-header={{this.small_header}}
	data-tabs={{this.tabs}}
	data-width={{this.width}}
	{{did-insert this.didCreate}}
	{{did-update this.didChange
		@adapt_container_width
		@height
		@hide_cover
		@hide_cta
		@href
		@lazy
		@show_facepile
		@small_header
		@tabs
		@width
	}}
></div>