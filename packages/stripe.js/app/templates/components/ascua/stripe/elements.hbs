<div {{did-insert this.setup}}>

	{{#if this.elements}}

		{{yield (augment this.elements
			card=(component "ascua/stripe/elements/card"
				elements=this.elements
			)
			cardNumber=(component "ascua/stripe/elements/cardNumber"
				elements=this.elements
			)
			cardExpiry=(component "ascua/stripe/elements/cardExpiry"
				elements=this.elements
			)
			cardCvc=(component "ascua/stripe/elements/cardCvc"
				elements=this.elements
			)
			iban=(component "ascua/stripe/elements/iban"
				elements=this.elements
			)
			idealBank=(component "ascua/stripe/elements/idealBank"
				elements=this.elements
			)
			paymentRequestButton=(component "ascua/stripe/elements/paymentRequestButton"
				elements=this.elements
			)
		)}}

	{{/if}}

</div>