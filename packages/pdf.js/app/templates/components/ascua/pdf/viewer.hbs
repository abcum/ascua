<app-pdf-viewer {{did-insert this.didCreate}} {{did-resize this.didResize}} {{did-update this.didChange @doc}} {{will-destroy this.willDelete}}>

	<img alt> <canvas loaded={{this.process.isSuccess}}></canvas>

	{{#if this.process.isRunning}}
		<app-pdf-loader>
			<div></div>
			<div></div>
		</app-pdf-loader>
	{{/if}}

</app-pdf-viewer>