<app-pdf-icons tabindex="-1" {{did-resize this.didResize}} {{did-insert this.didCreate}}>
	{{#each @pages as |page|}}
		<Ascua::pdf::icons::icon
			@w={{this.w}}
			@h={{this.h}}
			@pdf={{@pdf}}
			@page={{page}}
			@selected={{@page}}
			{{on "click" (fn @on-select page.pageNumber)}}
		/>
	{{/each}}
</app-pdf-icons>