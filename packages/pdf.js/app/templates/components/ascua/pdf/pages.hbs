<app-pdf-pages tabindex="-1" {{did-resize this.didResize}} {{did-insert this.didCreate}}>
	{{#each @pages as |page|}}
		<Ascua::pdf::pages::page
			@w={{this.w}}
			@h={{this.h}}
			@page={{page}}
			@selected={{@page}}
			@on-select={{fn @on-select page.pageNumber}}
		/>
	{{/each}}
</app-pdf-pages>