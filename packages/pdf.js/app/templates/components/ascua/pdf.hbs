{{on-press "up" this.prev}}
{{on-press "left" this.prev}}
{{on-press "down" this.next}}
{{on-press "right" this.next}}

<app-pdf {{did-insert this.didCreate}} {{did-update this.didChange @url @time}} {{will-destroy this.willDelete}}>

	<Ascua::pdf::pages @pages={{this.pages}} @page={{this.page}} @on-select={{this.select}} />

	<Ascua::pdf::icons @pages={{this.pages}} @page={{this.page}} @on-select={{this.select}} />

</app-pdf>