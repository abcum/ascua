			<Navigate @route="helpers.routing" @name="Routing" />
			<Navigate @route="helpers.strings" @name="Strings" />
		</section>

	{{/if}}

	{{#if (is-active "libraries")}}

		<div>apexcharts.js</div>
		<section show=true>
			<Navigate @route="libraries.apexcharts" @name="Setup" />
			<Navigate @route="libraries.apexcharts.elements" @name="Elements" />
		</section>

		<div>chart.js</div>
		<section show=true>
			<Navigate @route="libraries.chart" @name="Setup" />
