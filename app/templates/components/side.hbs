			<Navigate @route="elements.tooltip" @name="Tooltip" />
			<Navigate @route="elements.uploader" @name="Uploader" />
			<Navigate @route="elements.webview" @name="Webview" />
		</section>

	{{/if}}

	{{#if (is-active "helpers")}}

		<div>General</div>
		<section show={{begs-with (route-name) "helpers"}}>
			<Navigate @route="helpers.arrays" @name="Arrays" />
