<nav>

	{{#if (is-active "index")}}

		<div>Introduction</div>
		<Navigate @route="index" @name="Setup" />

	{{/if}}

	{{#if (is-active "guides")}}

		<div>General</div>
		<Navigate @route="guides.styling" @name="Styling" />
		<Navigate @route="guides.electron" @name="Electron" />

